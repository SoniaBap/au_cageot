{% extends 'base.html.twig' %}

{% block title %}{{ 'New Program' }}{% endblock %}

{% block body %}
  <div id="new-program-page">
		{{ form_start(form) }}
		{{ form_row(form.city) }}
		{{ form_row(form.name) }}
		{{ form_row(form.description) }}
	<div id="datepicker">
		{{ form_row(form.created_at) }}
	</div>
		{{ form_row(form.journey_of_reservation) }}
		{{ form_row(form.submit) }}
		{{ form_end(form) }}
  </div>
{% endblock %}
