{% extends 'base.html.twig' %}

{% block title %}
	{{ 'Register'|trans }}
{% endblock %}

{% block body %}
	<div id='user-register-page'>
		{{ form_start(form) }}
		{{ form_row(form.email) }}
		{{ form_row(form.plainPassword) }}
		{{ form_row(form.firstname) }}
		{{ form_row(form.lastname) }}
		{{ form_row(form.nickname) }}
		{{ form_row(form.submit) }}
		{{ form_end(form) }}
	</div>
{% endblock %}
